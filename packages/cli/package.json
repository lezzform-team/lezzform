{
  "name": "@lezzform/cli",
  "version": "0.2.9",
  "main": "bin/index.js",
  "license": "MIT",
  "type": "module",
  "types": "./bin/index.d.ts",
  "scripts": {
    "dev": "tsup && node bin/index.js dev -u http://localhost:3001",
