    "@types/readline-sync": "^1.4.8",
    "nodemon": "^3.0.1",
    "plop": "^4.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.1"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "chalk": "4",
    "commander": "^11.1.0",
    "esbuild": "^0.19.11",
    "prettier": "^3.2.3",
    "prompts": "^2.4.2",
    "socket.io-client": "^4.7.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
